<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mob2_front">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_register" />
        <activity android:name=".activity_add_restaurant" />
        <activity android:name=".activity_add_menu" />
        <activity android:name=".activity_show_restaurant" />
        <activity android:name=".activity_show_menu" />
        <activity android:name=".DetailRestaurant" />
        <activity android:name=".DetailMenu" />
        <activity android:name=".activity_update_restaurant" />
        <activity android:name=".activity_update_menu" />

    </application>

</manifest>